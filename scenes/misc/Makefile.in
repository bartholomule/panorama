
include @top_srcdir@/Makefile.vars

FILES = ./cornell_box.rt \
	./plastic_cornell_box.rt \
        ./projector_cartoon.rt

all:

install:
		@echo "Installing scenes (misc)..."
		@@top_srcdir@/mkinstalldirs ${TOPDIR}/scenes/misc
		@set -e; \
		for i in $(FILES); do \
		  @INSTALL_DATA@ $$i ${TOPDIR}/scenes/misc; \
		done

clean:

distclean:
		@rm -f Makefile
		@rm -f *.tga
