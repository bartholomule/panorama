
BUILD_TOP = ../..

include ${BUILD_TOP}/Makefile.vars

INSTALL = @INSTALL@
SRC_DIR = @srcdir@

FILES = atmosphere.rt

all:

install:
		@echo "Installing scenes (atmospheric)..."
		@${SRC_DIR}/${BUILD_TOP}/mkinstalldirs ${SCENE_DIR}/atmospheric
		@set -e; \
		for i in $(FILES); do \
		  @INSTALL_DATA@ ${SRC_DIR}/$$i ${SCENE_DIR}/atmospheric; \
		done

clean:

distclean:
		@rm -f Makefile
		@rm -f *.tga *.png *.jpg
