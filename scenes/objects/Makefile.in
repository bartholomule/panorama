
include @top_srcdir@/Makefile.vars

FILES = ./box.rt \
	./circle.rt \
        ./cone.rt \
        ./cylinder.rt \
        ./phong_triangle.rt \
        ./rectangle.rt \
        ./torus.rt \
        ./triangle.rt

all:

install:
		@echo "Installing scenes (objects)..."
		@@top_srcdir@/mkinstalldirs ${TOPDIR}/scenes/objects
		@set -e; \
		for i in $(FILES); do \
		  @INSTALL_DATA@ $$i ${TOPDIR}/scenes/objects; \
		done

clean:

distclean:
		@rm -f Makefile
		@rm -f *.tga
