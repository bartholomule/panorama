define c Camera
{
  translate ( <500, 400, 1200> );
  look_at   = <0,   100,    0>;
};

define l Light
{
  translate ( <500, 500, 500> );
  color = CWhite;
};

define l2 Light
{
  translate ( <0, 100, 500> );
  color = CWhite;
};


plane
{
