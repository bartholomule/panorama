
BUILD_TOP = ../..

include ${BUILD_TOP}/Makefile.vars

INSTALL = @INSTALL@
SRC_DIR = @srcdir@

FILES = C++_Coding_Style \
	General \
        PROJECTS \
        README
        
all:

install:
		@echo Installing developers documentation files...
		@set -e; \
		for i in $(FILES); do \
		  @INSTALL_DATA@ ${SRC_DIR}/$$i ${DOC_DIR}/developers; \
		done

clean:

distclean:
		@rm -f Makefile
